{
  "name": "pay-now/paynow-woocommerce",
  "description": "paynow.pl plugin for WooCommerce",
  "license": "GPLv3",
  "version": "2.5.7",
  "authors": [
    {
      "name": "mElements S.A.",
      "email": "kontakt@paynow.pl"
    }
  ],
  "type": "woocommerce-plugin",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "pay-now/paynow-php-sdk": "2.4.3",
    "nyholm/psr7": "^1.2",
    "php-http/curl-client": "^2.1",
    "cweagans/composer-patches": "^1.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "woocommerce/woocommerce-sniffs": "^0.1.1",
    "symfony/var-dumper": "^4.4",
    "symplify/vendor-patches": "^11.4"
  },
  "scripts": {
    "cs": [
      "vendor/bin/phpcs --ignore=includes/templates/*"
    ],
    "cs-fix": [
      "php vendor/bin/phpcbf"
    ]
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": false,
      "php-http/discovery": true
    }
  },
  "extra": {
    "patches": {
      "pay-now/paynow-php-sdk": [
        "patches/pay-now-paynow-php-sdk-src-paynow-model-paymentmethods-type-php.patch"
      ]
    },
    "hooks": {
      "pre-commit": [
        "php vendor/bin/phpcbf"
      ]
    }
  }
}
